<suite name="Regression Testing" >

	<listeners>
		<listener class-name="ca.amazon.utilities.TestUtils"/>
		<listener class-name="ca.amazon.utilities.FailTestUtils"/>
	</listeners>

	<test name="SignInProcess" allow-return-values="true">
		<classes>
			<class name="ca.amazon.TestSignIn" />
			<class name="ca.amazon.TestSignInRedirect" />
			<class name="ca.amazon.TestHomePage" />
		</classes>
	</test>
</suite>

       